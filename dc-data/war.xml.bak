    <str name="umdm">&lt;descMeta&gt;&lt;mediaType type="movingImage"&gt;&lt;form type="analog"&gt;feature&lt;/form&gt;&lt;/mediaType&gt;&lt;title type="main"&gt;Ancient civilizations The Vikings&lt;/title&gt;&lt;title type="alternate"&gt;Vikings&lt;/title&gt;&lt;agent type="provider"&gt;&lt;corpName&gt;Eagle Rock Entertainment&lt;/corpName&gt;&lt;/agent&gt;&lt;agent type="provider"&gt;&lt;corpName&gt;Cromwell Productions&lt;/corpName&gt;&lt;/agent&gt;&lt;agent type="provider"&gt;&lt;corpName&gt;Films for the Humanities (Firm)&lt;/corpName&gt;&lt;/agent&gt;&lt;covPlace&gt;&lt;geogName type="continent"&gt;North America&lt;/geogName&gt;&lt;geogName type="country"&gt;United States of America&lt;/geogName&gt;&lt;/covPlace&gt;&lt;covTime&gt;&lt;century era="ad"&gt;1901-2000&lt;/century&gt;&lt;date era="ad"&gt;1996&lt;/date&gt;&lt;/covTime&gt;&lt;culture&gt;not captured&lt;/culture&gt;&lt;language&gt;en&lt;/language&gt;&lt;description type="summary"&gt;Studies the effects of Viking aggressions on the history of Europe, their attacks upon the Church, their art and society and the activity of the Viking longboats in the development of European trade.&lt;/description&gt;&lt;subject type="browse"&gt;War, Military&lt;/subject&gt;&lt;subject label="Films@UM" type="browse"&gt;Ancient Cultures&lt;/subject&gt;&lt;subject label="Films@UM" type="browse"&gt;War and International Conflicts&lt;/subject&gt;&lt;subject scheme="LCSH" type="topical"&gt;Civilization, Viking&lt;/subject&gt;&lt;subject scheme="LCSH" type="topical"&gt;Viking antiquities&lt;/subject&gt;&lt;subject scheme="LCSH" type="topical"&gt;Civilization, Ancient&lt;/subject&gt;&lt;subject scheme="LCSH" type="topical"&gt;Vikings History&lt;/subject&gt;&lt;subject scheme="LCSH" type="topical"&gt;Viking ships&lt;/subject&gt;&lt;identifier type="oclc"&gt;ocm41234499&lt;/identifier&gt;&lt;identifier type="aleph"&gt;alephsys002497309&lt;/identifier&gt;&lt;identifier type="publisher"&gt;FFH 8691&lt;/identifier&gt;&lt;physDesc&gt;